{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "useBuiltIns": true,
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": false,
        "regenerator": true
      }
    ],
    [
      "@emotion/babel-plugin",
      {
        "sourceMap": true,
        "cssPropOptimization": true
      }
    ]
  ]
}
